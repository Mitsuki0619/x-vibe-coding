# サーバー設定
PORT=8080
ENV=development

# データベース設定
DATABASE_URL=host=localhost user=sns_user password=sns_password dbname=sns_db port=5432 sslmode=disable

# テスト用データベース設定
TEST_DATABASE_URL=host=localhost user=sns_test_user password=sns_test_password dbname=sns_test_db port=5433 sslmode=disable

# JWT設定（必須：本番環境では強力なキーに変更）
JWT_SECRET=your-secret-key-change-in-production

# CORS設定
CORS_ORIGINS=http://localhost:3000,http://localhost:19000

# ログレベル
LOG_LEVEL=info